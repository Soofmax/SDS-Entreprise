  "screenshots": [
    {
      "src": "/screenshots/desktop-home.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Page d'accueil - Version desktop"
    },
    {
      "src": "/screenshots/mobile-home.png",
      "sizes": "375x667",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Page d'accueil - Version mobile"
    },
    {
      "src": "/screenshots/desktop-services.png",
      "sizes": "1280x720",
      "type": "image/png",
